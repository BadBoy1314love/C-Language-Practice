#include<stdio.h>

int main()
{
	const int Pass = 60;
	int Score;

	printf("Please enter your score:");
	scanf_s("%d", &Score);

	printf("The score you entered is %d\n", Score);
	if (Score < Pass)
	{
		printf("I'm sorry you didn't pass the exam");
	}
	else
	{
		printf("Congratulations,you've passed the grade");
	}
}